{
  "name": "portfolio26-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "compress:videos": "node scripts/compress-videos.js",
    "compress:images": "node scripts/compress-images.js",
    "generate:backgrounds": "node scripts/generate-fast-optimized.js",
    "generate:backgrounds:force": "node scripts/generate-fast-optimized.js --force",
    "generate:backgrounds:single": "node scripts/generate-fast-optimized.js --single-digits --force",
    "cdn:sync": "aws s3 sync ./public/keypad-backgrounds s3://portfolio26-keypad-backgrounds/keypad-backgrounds --profile cdn-uploader --delete --content-type image/png --cache-control 'public,max-age=31536000,immutable' --exclude '.glyphs/*' --exclude '.gitkeep'",
    "cdn:sync:manifest": "aws s3 cp ./public/keypad-backgrounds/manifest.json s3://portfolio26-keypad-backgrounds/keypad-backgrounds/manifest.json --profile cdn-uploader --content-type application/json --cache-control 'public,max-age=3600'",
    "cdn:sync:images": "aws s3 sync ./public/images s3://portfolio26-keypad-backgrounds/images --profile cdn-uploader --delete --cache-control 'public,max-age=31536000,immutable' --exclude '.compressed-images.json'",
    "cdn:sync:videos": "aws s3 sync ./public/videos s3://portfolio26-keypad-backgrounds/videos --profile cdn-uploader --delete --cache-control 'public,max-age=31536000,immutable'",
    "cdn:sync:media-manifest": "aws s3 cp ./public/media-manifest.json s3://portfolio26-keypad-backgrounds/media-manifest.json --profile cdn-uploader --content-type application/json --cache-control 'public,max-age=3600'",
    "cdn:sync:media": "npm run cdn:sync:images && npm run cdn:sync:videos && npm run cdn:sync:media-manifest",
    "cdn:sync:all": "npm run cdn:sync && npm run cdn:sync:manifest && npm run cdn:sync:media",
    "media:prepare": "node scripts/generate-media-manifest.js",
    "media:deploy": "npm run media:prepare && npm run cdn:sync:media",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "screenshots": "npx playwright test tests/visual/screenshots.spec.js",
    "test:fonts": "node scripts/test-fonts.mjs",
    "test:fonts:adobe": "node scripts/test-adobe-fonts.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "lenis": "^1.3.13",
    "lucide-vue-next": "^0.546.0",
    "mixpanel-browser": "^2.71.0",
    "motion-v": "^1.7.3",
    "serve": "^14.2.4",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/devtools": "^8.0.2",
    "cli-progress": "^3.12.0",
    "husky": "^9.1.7",
    "puppeteer": "^24.24.1",
    "sharp": "^0.34.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.6.4"
  }
}
